<h1>Listings#new</h1>
<%= simple_form_for(@listing) do |f| %>
  <%= f.label :vehicle_name %>
  <%= f.text_field :vehicle_name %>
  <%= f.label :description %>
  <%= f.text_field :description %>
  <%= f.label :location %>
  <%= f.text_field :location %>
  <%= f.label :capacity %>
  <%= f.text_field :capacity %>
  <%= f.label :price %>
  <%= f.number_field :price %>
  <%= f.submit %>
<% end %>
