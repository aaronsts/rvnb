<h1 class="text-center">Listings near you!</h1>
<div class="d-flex">
  <div class="card-grid">
    <% @listings.each do |listing| %>
      <%= link_to listing_path(listing) do %>
        <%= render "card", listing: listing %>
      <% end %>
    <% end %>
  </div>
  <div id="map"
    data-markers="<%= @markers.to_json %>"
    data-mapbox-api-key="<%= ENV['MAPBOX_API_KEY'] %>">
  </div>
</div>
